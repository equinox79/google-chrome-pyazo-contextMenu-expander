 {
    "name":"pyazo-contextMenu-expander",
    "version":"0.0.2",
    "manifest_version": 2,
    "description":"Expand the google chrome context menu for pyazo",
    "permissions":[
        "http://yairc.cfe.jp:5000/*",
        "contextMenus"
    ],
    "icons": {
        "16": "icon/16.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/main.js"],
            "run_at": "document_start"
        }
    ],
    "background": {
        "scripts": ["js/main.js", "vendor/js/jquery-2.0.3.min.js"]
    }
}
